  "mjml": "<mjml>\n<mj-body>\n  \n    <mj-section padding-bottom=\"0px\">\n        <mj-column>\n            <mj-image width=\"100px\" src=\"{{ Branding.Logo.Url }}\"></mj-image>\n            <mj-divider border-color=\"{{ Branding.Theme.Accent }}\"></mj-divider>\n            <mj-text>\n                <p style=\"font-size: 20px\">\n                    Hi {{ TechName }},\n                </p>\n\n                <p style=\"line-height: 1.5\">\n                    <b>{{ ChatRequestorName }}</b> has started a chat session.\n                </p>\n                \n                <p style=\"line-height: 1.5\">\n                    Ticket: <b>#{{ TicketId }}</b> - <b>{{ TicketName }}</b><br>\n                    Company: <b>{{ ChatRequestorCompanyName }}</b>\n                </p>\n            </mj-text> \n        </mj-column>\n    </mj-section>\n  \n    <mj-section padding-top=\"0px\">\n        <mj-column>\n            <mj-button font-family=\"Helvetica\" background-color=\"{{ Branding.Theme.Primary }}\" color=\"white\" href=\"{{ Links.Tech }}/{{ TicketId }}\">\n                Go to ticket\n            </mj-button>\n        </mj-column>\n    </mj-section>\n  \n    <mj-section padding-top=\"0px\">\n      <mj-column>\n        <mj-text font-size=\"10px\">\n            Sent from Power Automate.\n        </mj-text>\n      </mj-column>\n    </mj-section>\n  \n</mj-body>\n</mjml>",
